@using Volo.Abp.AspNetCore.Mvc.UI.Theme.Shared.Toolbars
@model Toolbar
@{
    int i = 0;
    string mar = "";
}
@foreach (var toolbarItem in Model.Items.OrderBy(i => i.Order))
{
    mar = (i++ == 0)? "margin-top: 170px" : "";
    <i style="@mar;color:#ffffff;" class='nav-item fa fa-log-out bx-rotate-180'>@(await Component.InvokeAsync(toolbarItem.ComponentType))</i>
}